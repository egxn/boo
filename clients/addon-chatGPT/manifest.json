{
  "manifest_version": 2,
  "name": "Boo",
  "version": "1.0",
  "description": "Text to speech and speech to text for chatGPT",
  "content_scripts": [{
      "matches": ["*://chat.openai.com/*"],
      "js": ["boo.js"]
    }
  ],
  "permissions": [
    "<all_urls>",
    "webRequest",
    "http://localhost:5000/api/*"
  ]
}
